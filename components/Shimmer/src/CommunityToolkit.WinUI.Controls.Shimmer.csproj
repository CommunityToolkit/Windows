<Project Sdk="MSBuild.Sdk.Extras/3.0.23">
  <PropertyGroup>
    <ToolkitComponentName>Shimmer</ToolkitComponentName>
    <Description>This package contains Shimmer.</Description>

    <!-- Rns suffix is required for namespaces shared across projects. See https://github.com/CommunityToolkit/Labs-Windows/issues/152 -->
    <RootNamespace>CommunityToolkit.WinUI.Controls.ShimmerRns</RootNamespace>
  </PropertyGroup>


<ItemGroup>
    <ProjectReference Include="..\..\Animations\src\CommunityToolkit.WinUI.Animations.csproj"></ProjectReference>
  </ItemGroup>

  <!-- Sets this up as a toolkit component's source project -->
  <Import Project="$(ToolingDirectory)\ToolkitComponent.SourceProject.props" />
</Project>
